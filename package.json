{
  "name": "reportbuilder",
  "version": "1.0.0",
  "description": "Small tool to build a report",
  "main": "./lib/index.ts",
  "types": "./lib/index.ts",
  "scripts": {
    "build:ts": "./node_modules/.bin/ts-node ./src/index.ts",
	"prince": "prince --style=out/report.css out/report.html --output=out/report.pdf",
    "start": "npm run build:ts && npm run prince"
  },
  "repository": {
	  "type": "git",
	  "url": "https://github.com/MonsieurMan/ReportBuilder"
  },
  "keywords": [
    "report",
    "internship",
    "prince"
  ],
  "devDependencies": {
    "@types/jsdom": "^11.0.0",
    "@types/node": "7.0.28",
    "jsdom": "^11.0.0",
    "ts-loader": "2.1.0",
    "ts-node": "3.0.6",
    "typescript": "2.3.4"
  },
  "author": "Ange Picard",
  "license": "ISC"
}
