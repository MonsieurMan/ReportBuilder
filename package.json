{
  "name": "reportbuilder",
  "version": "0.0.1",
  "description": "Small tool to build a report",
  "main": "./lib/index.ts",
  "types": "./lib/index.ts",
  "scripts": {
    "gen-toc-tof": "ts-node ./src/index.ts ./out/report.html ./out/report.html",
    "parse:md": "marked ./files/report.md -o ./out/report.html",
    "parse:pdf": "weasyprint out/report.html out/report.pdf -s out/style.css",
    "start": "npm run parse:md && npm run gen-toc-tof && npm run parse:pdf"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MonsieurMan/ReportBuilder"
  },
  "keywords": [
    "report",
    "internship",
    "prince"
  ],
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/gulp": "^4.0.5",
    "@types/jsdom": "^11.0.0",
    "@types/marked": "^0.4.0",
    "@types/node": "7.0.28",
    "@types/through2": "^2.0.33",
    "fs-extra": "^7.0.0",
    "gulp": "^4.0.0",
    "gulp-cached": "^1.1.1",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.1",
    "jsdom": "^11.0.0",
    "marked": "^0.4.0",
    "through2": "^2.0.3",
    "ts-loader": "2.1.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "author": "Ange Picard",
  "license": "ISC"
}
